{% extends 'base.html.twig' %}

{% block title %}Hello VoitureController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>
    <div class="example-wrapper">
    <h1>Informations d'une offre d'emploi</h1>
<ul>
{% for d in demande %}
<li> ID : {{ d.id|e }} </li>
<li> Titre Demande : {{d.titre|e }} </li>
<li> Diplome demandé : {{d.diplome|e }} </li>
<li> Technologies utilisées : {{d.technologie|e }} </li>

{% endfor %}
<div>
<button type="button" class="btn btn-light"><a href={{path('demande')}}>Back</a></button>
</div>
</ul>
</div> 
{% endblock %}
